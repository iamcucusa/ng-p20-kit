{
  "name": "ng-p20-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve showcase",
    "build-showcase": "ng build showcase",
    "watch-showcase": "ng build showcase --watch --configuration development",
    "test": "ng test",
    "docs:serve:showcase": "compodoc -p projects/showcase/tsconfig.app.json -s -w --port 8081",
    "docs:build:showcase": "compodoc -p projects/showcase/tsconfig.app.json -d ./docs-showcase --theme material",
    "docs:json:showcase": "compodoc -p projects/showcase/tsconfig.app.json -d ./.compodoc-showcase -e json --silent",
    "theme:generate": "tsx projects/theme/src/lib/scripts/generate-theme-assets.ts",
    "generate-spacing": "node projects/theme/src/lib/scripts/generate-spacing-scss.js",
    "generate-typography": "node projects/theme/src/lib/scripts/generate-typography.js",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@ngrx/effects": "^20.0.1",
    "@ngrx/router-store": "^20.0.1",
    "@ngrx/signals": "^20.0.1",
    "@ngrx/store": "^20.0.1",
    "@ngrx/store-devtools": "^20.0.1",
    "@primeuix/styles": "^1.2.5",
    "@primeuix/themes": "^1.2.5",
    "date-fns": "^4.1.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.3",
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.3",
    "@compodoc/compodoc": "^1.1.30",
    "@ngrx/eslint-plugin": "^20.0.1",
    "@ngrx/schematics": "^20.0.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.4.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.0"
  }
}
